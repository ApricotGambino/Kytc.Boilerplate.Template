{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../../../../src/",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "generated/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml,*.swagger.json}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "generated/_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "Kytc.Boilerplate.Template",
      "_appTitle": "Kytc.Boilerplate.Template",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_gitContribute": null,
      "pdf": false
    }
  }
}
